			  /*System.out.print(s+"\n");*/
			   i++; tg1=s;j=0;
			   while (tg1.length()!=0)
				{
					
					tg1=tg1+" ";
					
					
					k1=tg1.indexOf(" ");
					
			
					s1= tg1.substring(0,k1);
					
					j++;
					x[i][j]=Double.parseDouble(s1);
					
					
					tg1=tg1.substring(k1,tg1.length()); tg1=tg1.trim();
					
				}		   
			   
		       }
		  	   
