		for (int i1=1;i1<=m1;i1++)
			for (int j1=1;j1<=n1;j1++)
		      if (distance(kq[i][i1],kq[j][j1])< min)
		    	  min=distance(kq[i][i1],kq[j][j1]);		      
		
		return min;		
	 }*/
	
	
	public static void main(String[] args)  throws Exception 
